<h1>Categories</h1>
<%= link_to "New Category", new_category_path %>
<hr>

<% @categories.each do |category| %>
  <div>
    <%= link_to category.name, category_path(category) %>
    <%= link_to "Edit", edit_category_path(category) %> <%= link_to "Delete", category_path(category), method: :delete %></div>

<% end %>
